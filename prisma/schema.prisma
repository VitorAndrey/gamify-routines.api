generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model plannings {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model tasks {
  /// Multiple data types found: String (ObjectId): 91.7%, Int: 8.3% out of 12 sampled entries
  id    Json   @id @map("_id")
  name  String
  /// Multiple data types found: Float: 16.7%, Int: 83.3% out of 6 sampled entries
  price Json?
}
